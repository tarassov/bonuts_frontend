{
	"name": "bonuts",
	"version": "0.6.0",
	"homepage": "https://tarassov.github.io/bonuts_frontend/",
	"dependencies": {
		"@material-ui/core": "^4.12.3",
		"@material-ui/icons": "^4.11.2",
		"@material-ui/styles": "^4.11.4",
		"@redux-devtools/extension": "^3.2.2",
		"@testing-library/jest-dom": "^5.16.2",
		"@testing-library/react": "^12.1.3",
		"@testing-library/user-event": "^13.5.0",
		"chart.js": "^2.9.4",
		"classnames": "^2.3.1",
		"downshift": "^3.4.8",
		"history": "5.1.0",
		"i18next": "^21.6.11",
		"i18next-browser-languagedetector": "^6.1.3",
		"i18next-http-backend": "^1.3.2",
		"match-sorter": "^6.3.1",
		"node-sass": "7.0.1",
		"notistack": "^1.0.10",
		"perfect-scrollbar": "^1.5.2",
		"pluralize": "^7.0.0",
		"prop-types": "^15.6.2",
		"query-string": "^6.14.1",
		"react": "^17.0.2",
		"react-chartjs-2": "^2.11.2",
		"react-datetime": "^3.1.1",
		"react-dom": "^17.0.2",
		"react-dropzone": "^11.4.2",
		"react-i18next": "^11.15.4",
		"react-qr-svg": "^2.4.0",
		"react-redux": "^7.2.5",
		"react-router-dom": "^6.2.1",
		"react-scripts": "5.0.0",
		"react-table": "^7.7.0",
		"react-to-print": "^2.13.0",
		"react-user-avatar": "^1.10.0",
		"redux": "^4.1.2",
		"redux-first-history": "^5.0.1",
		"redux-form": "^8.3.7",
		"redux-persist": "^5.10.0",
		"redux-subscriber": "^1.1.0",
		"redux-thunk": "^2.3.0",
		"typeface-roboto": "0.0.54"
	},
	"scripts": {
		"start": "PORT=3001 react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"dev-build": "NODE_PATH=./src npm run build",
		"install:clean": "rm -rf node_modules/ && rm -rf package-lock.json && npm install && npm start",
		"compile-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.css",
		"minify-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.min.css --output-style compressed",
		"map-sass": "node-sass src/assets/scss/material-dashboard-pro-react.scss src/assets/css/material-dashboard-pro-react.css --source-map true",
		"eslint": "eslint ./src",
		"eslint:fix": "eslint ./src --fix",
		"prettier": "prettier --check src/**/*.{js,jsx,ts,tsx} --config ./.prettierrc",
		"prettier:fix": "prettier --write src/**/*.{js,jsx,ts,tsx} --config ./.prettierrc",
		"stylelint": "stylelint src/**/*.css",
		"stylelint:fix": "stylelint src/**/*.css --fix",
		"format:check": "eslint ./src && prettier --check src/**/*.{js,jsx,ts,tsx} --config ./.prettierrc && stylelint src/**/*.css",
		"format:fix": "eslint ./src --fix && prettier --write src/**/*.{js,jsx,ts,tsx} --config ./.prettierrc && stylelint src/**/*.css --fix",
		"predeploy": "npm run build",
		"deploy": "gh-pages -b master -d build"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier": "^4.0.0",
		"gh-pages": "^4.0.0",
		"prettier": "^2.6.2"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	}
}
